{
  "name": "forval",
  "description": "A swagger client generator for typescript",
  "version": "6.11.3",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "forval": "dist/bin/orval.js"
  },
  "main": "dist/index.js",
  "keywords": [
    "rest",
    "client",
    "swagger",
    "open-api",
    "fetch",
    "data fetching",
    "code-generation",
    "angular",
    "react",
    "react-query",
    "svelte",
    "svelte-query",
    "vue",
    "vue-query",
    "msw",
    "mock",
    "axios",
    "vue-query",
    "vue",
    "swr"
  ],
  "authors": [
    {
      "name": "Victor Bury",
      "email": "bury.victor@gmail.com"
    },
    {
      "name": "Juan Aguilar Santillana",
      "email": "mhpoin@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/botika/forval"
  },
  "scripts": {
    "fix": "pnpm lint && pnpm fmt",
    "check": "pnpm lint:check && pnpm fmt:check",
    "lint": "eslint --cache --max-warnings 0 --fix '**/*.{ts,js}'",
    "fmt": "prettier --write '**/*.{md,prisma,json}'",
    "lint:check": "eslint --cache --max-warnings 0 '**/*.{ts,js}'",
    "fmt:check": "prettier --check '**/*.{md,prisma,json}'",
    "build": "rm -fr dist && tsc -p tsconfig.build.json",
    "test": "vitest --run test.ts",
    "format": "prettier --write .",
    "format:staged": "pretty-quick --staged",
    "release": "pnpm test && dotenv release-it",
    "generate-api": "node ./dist/bin/orval.js --config ./samples/react-query/basic/orval.config.ts --watch",
    "prepare": "husky install",
    "commitlint": "commitlint",
    "update-samples": "zx ./scripts/update-samples.mjs",
    "prepublish": "pnpm build"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "acorn": "^8.7.1",
    "cac": "^6.7.12",
    "chalk": "^4.0.0",
    "chokidar": "^3.5.3",
    "compare-versions": "^4.1.3",
    "cuid": "^2.1.8",
    "debug": "^4.3.4",
    "esbuild": "^0.14.46",
    "esutils": "2.0.3",
    "execa": "^5.0.0",
    "find-up": "^5.0.0",
    "fs-extra": "^10.1.0",
    "globby": "^11.0.0",
    "ibm-openapi-validator": "^0.81.0",
    "inquirer": "^8.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "micromatch": "^4.0.5",
    "openapi3-ts": "^2.0.2",
    "swagger2openapi": "^7.0.8",
    "tsconfck": "^2.0.1",
    "upath": "^2.0.1",
    "url": "^0.11.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@faker-js/faker": "^7.2.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.omit": "^4.5.7",
    "@types/lodash.omitby": "^4.6.7",
    "@types/lodash.uniq": "^4.5.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/lodash.uniqwith": "^4.5.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/request": "^2.48.8",
    "@types/validator": "^13.7.3",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.0.0",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "release-it": "^15.0.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vitest": "^0.15.1",
    "zx": "^7.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint --cache --fix",
    "*.{json,md}": "prettier --write"
  }
}
